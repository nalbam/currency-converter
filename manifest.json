{
  "name": "Currency Converter",
  "description": "Multiple Currency Converter.",
  "version": "1.5.2",
  "manifest_version": 2,
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon48.png",
    "default_title": "Currency Converter",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "unlimitedStorage",
    "notifications",
    "http://*/*"
  ]
}
